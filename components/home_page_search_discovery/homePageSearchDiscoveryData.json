{
  "mainHead": "Search The National Archives and over 2,500 archives across the UK",
  "classNameContainer": "search-form-container",
  "id": "held-by-react-component",
  "form": {
    "method": "get",
    "action": "https://discovery.nationalarchives.gov.uk/results/r",
    "role": "search",
    "grid": {
      "container": "main-search-container",
      "group": {
        "fieldGroup": "field-group",
        "fieldSubGroup": "field-sub-group",
        "full": "full",
        "halfToHalf": "half-to-half"
      }
    },
    "fieldShow": {
      "label": {
        "text": "Show",
        "class": "fixed-width",
        "for": "search-all-collections"
      },
      "input": {
        "name": "_q",
        "type": "text",
        "req": "required",
        "ariaRequired": "true",
        "id": "search-all-collections",
        "placeholder": "everything that contains these terms"
      },
      "errorMsg": "Warning field show"
    },
    "fieldBetween": {
      "label": {
        "text": "between",
        "class": "fixed-width",
        "for": "start-date"
      },
      "input": {
        "id": "start-date",
        "name": "_sd",
        "placeholder": "YYYY",
        "type": "number",
        "class": "small"
      },
      "errorMsgLength": "Warning field between to long",
      "errorMsgCurrentYear": "Warning field between current year",
      "errorMsgDateRange": " Please enter both start date and end date"
    },
    "fieldAnd": {
      "label": {
        "text": "and",
        "for": "start-date"
      },
      "input": {
        "id": "end-date",
        "name": "_ed",
        "placeholder": "YYYY",
        "type": "number",
        "class": "small"
      },
      "errorMsgLength": "Warning field between to long",
      "errorMsgCurrentYear": "Warning field between current year",
      "errorMsgDateRange": " Please enter both start date and end date"
    },
    "fieldAcross": {
      "label": {
        "text": "across",
        "for": "held-by"
      },
      "select": {
        "id": "held-by",
        "class": "medium",
        "name": "_hb",
        "options": [
          { "name": "All records", "value": "", "id": 1 },
          { "name": "The National Archives only", "value": "tna", "id": 2 },
          { "name": "Other archives only", "value": "oth", "id": 3 }
        ]
      }
    },
    "inputSearch": {
      "type": "submit",
      "title": "Search",
      "value": "Search"
    }
  }
}
